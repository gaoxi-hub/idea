<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generate.TxDao">
  <resultMap id="BaseResultMap" type="generate.Tx">
    <result column="num" jdbcType="INTEGER" property="num" />
    <result column="id" jdbcType="INTEGER" property="id" />
  </resultMap>
  <insert id="insert" parameterType="generate.Tx">
    insert into tx (num, id)
    values (#{num,jdbcType=INTEGER}, #{id,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="generate.Tx">
    insert into tx
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="num != null">
        num,
      </if>
      <if test="id != null">
        id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="num != null">
        #{num,jdbcType=INTEGER},
      </if>
      <if test="id != null">
        #{id,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>